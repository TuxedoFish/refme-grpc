{
  "family": "refme-api",
  "executionRoleArn": "arn:aws:iam::050351529511:role/ecsInstanceRole",
  "containerDefinitions": [
    {
      "image": "050351529511.dkr.ecr.eu-west-2.amazonaws.com/refme-api-v2-container",
      "name": "refme-api-v2-container"
    },
    {
      "portMappings": [
        {
          "hostPort": 80,
          "protocol": "tcp",
          "containerPort": 80
        },
        {
          "hostPort": 8081,
          "protocol": "tcp",
          "containerPort": 8081
        }
      ],
      "image": "050351529511.dkr.ecr.eu-west-2.amazonaws.com/refme-envoy-container",
      "name": "refme-envoy-container"
    }
  ],
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512"
}