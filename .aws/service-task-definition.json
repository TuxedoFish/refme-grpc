{
  "family": "refme-api",
  "containerDefinitions": [
    {
      "image": "050351529511.dkr.ecr.eu-west-2.amazonaws.com/refme-api-v2-container",
      "name": "refme-api-v2-container",
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/service-task-definition/api",
          "awslogs-region": "eu-west-2",
          "awslogs-stream-prefix": "ecs"
        }
      }
    },
    {
      "portMappings": [
        {
          "hostPort": 80,
          "protocol": "tcp",
          "containerPort": 80
        },
        {
          "hostPort": 8081,
          "protocol": "tcp",
          "containerPort": 8081
        }
      ],
      "image": "050351529511.dkr.ecr.eu-west-2.amazonaws.com/refme-envoy-container",
      "name": "refme-envoy-container",
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/service-task-definition/envoy",
          "awslogs-region": "eu-west-2",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512"
}