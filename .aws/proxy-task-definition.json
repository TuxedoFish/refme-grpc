{
    "family": "envoy-proxy",
    "executionRoleArn": "arn:aws:iam::050351529511:role/ecsTaskExecutionRole",
    "containerDefinitions": [
      {
        "portMappings": [
          {
            "hostPort": 80,
            "protocol": "tcp",
            "containerPort": 80
          },
          {
            "hostPort": 8081,
            "protocol": "tcp",
            "containerPort": 8081
          }
        ],
        "image": "050351529511.dkr.ecr.eu-west-2.amazonaws.com/envoy-proxy-container",
        "name": "envoy-proxy-container",
        "logConfiguration": {
          "logDriver": "awslogs",
          "options": {
            "awslogs-group": "/ecs/proxy-task-definition/envoy",
            "awslogs-region": "eu-west-2",
            "awslogs-stream-prefix": "ecs"
          }
        },
        "environment": [
          {
            "name": "ENVOY_UID",
            "value": "0"
          }
        ]
      }
    ],
    "networkMode": "host",
    "cpu": "128",
    "memory": "256"
  }